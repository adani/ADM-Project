    <om:forProduct rdf:resource="urn:x-domain:oreilly.com:product:9781565924864.BOOK"/>
  </om:ProductBiography><om:ProductBiography rdf:about="http://people.oreilly.com/agent/urn:x-domain:oreilly.com:agent:pdb:314/product-bio/urn:x-domain:oreilly.com:product:9781565924864.IP" xml:lang="en" xmlns:om="http://purl.oreilly.com/ns/meta/">
