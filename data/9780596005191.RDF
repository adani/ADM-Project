    <om:byline xml:lang="en">By Chuck Cavaness, Brian Keeton</om:byline>
    
    <om:audience rdf:resource="http://purl.oreilly.com/audiences/"/>
    <marcrel:BKD rdf:resource="urn:x-domain:oreilly.com:agent:pdb:1222" xmlns:marcrel="http://www.loc.gov/loc.terms/relators/"/>
