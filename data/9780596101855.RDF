    <marcrel:EDT rdf:resource="urn:x-domain:oreilly.com:agent:pdb:205" xmlns:marcrel="http://www.loc.gov/loc.terms/relators/"/>
    
    
    <om:sellingPoints rdf:datatype="http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral">&lt;ol&gt;&lt;li&gt;Written by a Perforce insider &lt;/li&gt;&lt;li&gt;The first book on Perforce to hit the market&lt;/li&gt;&lt;li&gt;Focuses on the "why" and "when" of using Perforce, unlike the manuals (which focus on the "how")&lt;/li&gt;&lt;/ol&gt;</om:sellingPoints>
    <om:editionLabel xml:lang="en">1st Edition</om:editionLabel>
    <om:tagline xml:lang="en">Channeling the Flow of Change in Software Development Collaboration</om:tagline>
    <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime" xmlns:dc="http://purl.org/dc/terms/">2013-07-24T19:13:34.454422+00:00</dc:modified>
