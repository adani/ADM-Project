    <dc:title xml:lang="en" xmlns:dc="http://purl.org/dc/terms/">Microsoft Project 2007: The Missing Manual</dc:title>
    <marcrel:EDT rdf:resource="urn:x-domain:oreilly.com:agent:pdb:600" xmlns:marcrel="http://www.loc.gov/loc.terms/relators/"/>
    <dc:language xml:lang="en" xmlns:dc="http://purl.org/dc/terms/">en-US</dc:language>
    
