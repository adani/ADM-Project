    <om:sortableTitle xml:lang="en">excel 2000 in a nutshell</om:sortableTitle>
    <om:byline xml:lang="en">By Jinjer Simon</om:byline>
    <dc:format xml:lang="en" xmlns:dc="http://purl.org/dc/terms/">6 x 9 in</dc:format>
