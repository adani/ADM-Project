    <om:projectManager rdf:resource="urn:x-domain:oreilly.com:agent:pdb:1088"/>
    <om:videoPreview xml:lang="en"/>
    <marcrel:PBD rdf:resource="urn:x-domain:oreilly.com:agent:pdb:1061" xmlns:marcrel="http://www.loc.gov/loc.terms/relators/"/>
