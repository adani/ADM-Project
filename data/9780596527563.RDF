    <om:sortableTitle xml:lang="en">programming .net 3.5</om:sortableTitle>
    <dc:rightsHolder xml:lang="en" xmlns:dc="http://purl.org/dc/terms/">Jesse Liberty and Alex Horovitz</dc:rightsHolder>
