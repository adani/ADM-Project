    <dc:title xml:lang="en" xmlns:dc="http://purl.org/dc/terms/">The Photoshop CS4 Companion for Photographers</dc:title>
    
    <dc:created xml:lang="en" xmlns:dc="http://purl.org/dc/terms/">2009-03-20</dc:created>
