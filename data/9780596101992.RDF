    <om:byline xml:lang="en">By David Flanagan</om:byline>
    
    <dc:rightsHolder xml:lang="en" xmlns:dc="http://purl.org/dc/terms/">O'Reilly Media, Inc.</dc:rightsHolder>
    <om:projectManager rdf:resource="urn:x-domain:oreilly.com:agent:pdb:4050"/>
