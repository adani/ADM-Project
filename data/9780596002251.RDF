    <om:asciiTitle xml:lang="en">Embedding Perl in HTML with Mason</om:asciiTitle>
    
    <dc:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime" xmlns:dc="http://purl.org/dc/terms/">2013-07-25T03:05:52.545302+00:00</dc:modified>
