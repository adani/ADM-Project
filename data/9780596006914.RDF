    <om:asciiTitle xml:lang="en">Flash Out of the Box</om:asciiTitle>
    <om:beginUpcomingStatus rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2008-09-01T00:00:00</om:beginUpcomingStatus>
    
