    <dc:title xml:lang="en" xmlns:dc="http://purl.org/dc/terms/">Flash Out of the Box</dc:title>
    <om:endNewStatus rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2009-05-01T00:00:00</om:endNewStatus>
    <om:availableIn rdf:resource="http://purl.oreilly.com/channels/kindle"/>
    
