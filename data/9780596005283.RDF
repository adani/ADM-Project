    <dc:subject rdf:resource="http://purl.oreilly.com/onix/keywords/LPI%20Linux%20Certification%20in%20a%20Nutshell" xmlns:dc="http://purl.org/dc/terms/"/>
    <dc:subject rdf:resource="http://purl.oreilly.com/onix/keywords/certification%2C" xmlns:dc="http://purl.org/dc/terms/"/>
